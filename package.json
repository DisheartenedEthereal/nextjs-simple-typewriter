{
  "name": "react-simple-typewriter",
  "version": "2.0.1",
  "description": "A simple react component for adding a nice typewriter effect to your project.",
  "author": "awran5 <awran5@yahoo.com>",
  "license": "MIT",
  "repository": "https://github.com/awran5/react-simple-typewriter",
  "bugs": "https://github.com/awran5/react-simple-typewriter/issues",
  "homepage": "https://react-simple-typewriter.vercel.app/",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf dist",
    "start": "rollup -cw --exports auto",
    "build": "yarn clean && rollup -c --exports auto",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.52.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "typewriter",
    "typewriter effect",
    "react",
    "react typewriter",
    "react typewriter effect",
    "react-component"
  ],
  "tags": [
    "react",
    "typewriter",
    "react-component"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "-": "^0.0.1"
  }
}
